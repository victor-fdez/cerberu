<div class="page-header">
	<h1><%= @cheese.name %></h1>
</div>
<p id="notice"><%= notice %></p>
	<div class="row">
		<div class="col-md-12" style="margin-bottom: 15px">
			<%= button_tag(:type => 'button', :class => "btn btn-default" ) do %>
				<% link_to "Edit", edit_cheese_path %>
			<% end %>
			<%= button_tag(:type => 'button', :class => "btn btn-default" ) do %>
				<% link_to "Back", cheeses_path %>
			<% end %>
		</div>
	</div>
	<div class="row">
		<div class="col-md-12">
			<h4>about</h4>
			<p><%= @cheese.about %></p>
			<% if false %>
				<%= text_area_tag '', @cheese.about, :class => "tinymce" %>
				<%= tinymce :alternate %>
			<% end %>
		</div>
	</div>
	<div class="row">
		<div class="col-md-12">
			<h3>Process Records</h3>
			<% @cheese.process_records.each do |process_record| %>
				<%= render "process_record", :process_record => process_record %>
			<% end %>
		</div>
	</div>
	<div class="row">
	  <div class="col-md-12">
			<div class="pictures">
				<h4>pictures</h4>
			</div>
	  </div>
	</div>


	
